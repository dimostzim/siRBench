    "bagging_freq": 1,
    "lambda_l1": 0.9,
    "lambda_l2": 1.2,
    "min_gain_to_split": 0.0,
    "max_bin": 255,
    "n_estimators": 6000,
    "early_stopping_rounds": 400,
    "device_type": "gpu",
    "gpu_platform_id": 0,
    "gpu_device_id": 0,
    "verbosity": -1
  },
  "cross_validation": {
    "n_splits": 5,
    "shuffle": true,
    "random_state": 42
  },
  "elasticnet": {
    "alpha": 0.001,
    "l1_ratio": 0.5,
    "max_iter": 100000,
    "tol": 0.0001,
    "fit_intercept": true,
    "selection": "random"
  }
