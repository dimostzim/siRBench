{
  "random_seed": 42,
  "lightgbm": {
    "objective": "regression",
    "metric": "rmse",
    "boosting_type": "gbdt",
    "learning_rate": 0.015,
    "num_leaves": 28,
    "max_depth": -1,
    "min_data_in_leaf": 64,
    "min_sum_hessian_in_leaf": 5.0,
    "feature_fraction": 0.58,
